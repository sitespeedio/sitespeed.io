a#waterfall
h3 Waterfall

if (options.html.showAllWaterfallSummary === true)
  p Choose HAR:
  select#page-selector

#legend-holder

if options.html.fetchHARFiles
  include ./fetchHAR.pug
else
  include ./includeHARinHTML.pug

.downloads
  - var harEnding = options.gzipHAR ? '.har.gz' : '.har'
  - var harName = 'data/browsertime' + harEnding
  - var harDownloadName = downloadName + harEnding
  a.button.button-download(href=harName, download=harDownloadName ) Download HAR

  if options.html.logDownloadLink
    a.button.button-download(href='../../logs/sitespeed.io.log', download) Download run log
