
a#webpagetest
h2 WebPageTest

- const data = pageInfo.data.webpagetest.pageSummary[0].data
- const medianRun = data.median

a(href='#{data.summary}') WebPageTest

p !{data.from}

p #{medianRun.firstView.browser_name} - #{medianRun.firstView.browser_version}

each view in ['firstView', 'repeatView']
  - var median = medianRun[view];
  if median
    h4 #{view}
    table
      caption Timing metrics
      tr
        th TTFB
        th Render
        th fullyLoaded
        th SpeedIndex
      tr
        td #{median.TTFB}
        td #{median.render}
        td #{median.fullyLoaded}
        td #{median.SpeedIndex}
    h5 Waterfall
    img(src='#{median.images.waterfall}', alt='Waterfall view', class='u-max-full-width')
    h5 Connection view
    img(src='#{median.images.connectionView}', alt='Connnection view', class='u-max-full-width')
